<div class="blog-show">
  <%= link_to 'Back', blogs_path, class: 'btn btn-info' %>
  <h1><%= @blog.title %></h1>
  <p class="blog-footer">
    <ion-icon name="pricetag"></ion-icon>
    <span class="blog-category"><%= @blog.category.name%></span>
  </p>
  <%= render_md_content(@blog.content) %>
</div>